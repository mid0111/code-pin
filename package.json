{
  "name": "code-pin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack/build.webpack.js",
    "lint": "eslint server client test webpack --fix",
    "test": "npm-run-all test:*",
    "test:server": "istanbul cover --dir ./coverage/server/ _mocha test/server/**/*.spec.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "NODE_ENV=development DEBUG=code-horic:* nodemon ./bin/www",
    "dev:client": "webpack-dev-server --hot --inline --content-base client/ --config webpack/dev.webpack.js",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "^2.5.2",
    "express": "~4.13.4",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "serve-favicon": "~2.3.0"
  },
  "engines": {
    "node": ">=6.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^4.0.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "supertest": "^2.0.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
